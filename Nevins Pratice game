

Function RunGame(){
	let player1score = 26;
	let player2score = 26;
	let player1move;
	letplayer2move;

	while(){}
		player1Move = pullCard();
		player2move= pullCard ();
		console.log(player1move)


		if(player1move > player2move){
			player1score++;
			player2score--:
		}
		else{
			player1score--;
			player2score++:
		}
	}
}

function pullCard(){
	return math.floor(Math.random() * 13 + 1);
}

